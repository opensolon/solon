{
  "properties": [
    {
      "name": "server.port",
      "type": "java.lang.Integer",
      "defaultValue": 8080,
      "description": "服务端口"
    },
    {
      "name": "server.host",
      "type": "java.lang.String",
      "defaultValue": "0.0.0.0",
      "description": "服务主机"
    },
    {
      "name": "server.wrapPort",
      "type": "java.lang.Integer",
      "defaultValue": 8080,
      "description": "服务包装端口（通过容器运行时，且内外端口不一致时用）"
    },
    {
      "name": "server.wrapHost",
      "type": "java.lang.String",
      "defaultValue": "0.0.0.0",
      "description": "服务包装主机（通过容器运行时，且内外主机不一致时用）"
    },
    {
      "name": "server.contextPath",
      "type": "java.lang.String",
      "description": "服务上下文路径"
    },
    {
      "name": "server.request.maxBodySize",
      "type": "java.lang.String",
      "description": "设定最大的请求包大小（或表单项的值大小）"
    },
    {
      "name": "server.request.maxFileSize",
      "type": "java.lang.String",
      "description": "设定最大的上传文件大小"
    },
    {
      "name": "server.request.maxHeaderSize",
      "type": "java.lang.String",
      "description": "设定最大的请求头大小"
    },
    {
      "name": "server.request.encoding",
      "type": "java.lang.String",
      "defaultValue": "utf-8",
      "description": "设定请求体编码"
    },
    {
      "name": "server.response.encoding",
      "type": "java.lang.String",
      "defaultValue": "utf-8",
      "description": "设定响应体编码"
    },
    {
      "name": "server.session.timeout",
      "type": "java.lang.Integer",
      "defaultValue": 7200,
      "description": "设定会话超时秒数（单位：秒）"
    },
    {
      "name": "server.session.cookieName",
      "type": "java.lang.String",
      "description": "设定会话id的cookieName"
    },
    {
      "name": "server.session.state.domain",
      "type": "java.lang.String",
      "description": "设定会话状态的cookie域（默认为当前域名）"
    },
    {
      "name": "server.ssl.keyStore",
      "type": "java.lang.String",
      "description": "服务ssl证书（例：demo.jks 或 demo.pfx）"
    },
    {
      "name": "server.ssl.keyPassword",
      "type": "java.lang.String",
      "description": "服务ssl证书密码"
    },
    {
      "name": "solon.app.name",
      "type": "java.lang.String",
      "description": "应用名称"
    },
    {
      "name": "solon.app.group",
      "type": "java.lang.String",
      "description": "应用分组"
    },
    {
      "name": "solon.app.namespace",
      "type": "java.lang.String",
      "description": "应用命名空间"
    },
    {
      "name": "solon.app.title",
      "type": "java.lang.String",
      "description": "应用标题"
    },
    {
      "name": "solon.stop.safe",
      "type": "java.lang.Integer",
      "defaultValue": 0,
      "description": "应用是否安全停止"
    },
    {
      "name": "solon.stop.delay",
      "type": "java.lang.Integer",
      "defaultValue": 10,
      "description": "应用安全停止延时（单位：秒）"
    },
    {
      "name": "solon.extend",
      "type": "java.lang.String",
      "description": "应用体外扩展目录"
    },
    {
      "name": "solon.config",
      "type": "java.lang.String",
      "description": "应用体外扩展配置"
    },
    {
      "name": "solon.output.meta",
      "type": "java.lang.Integer",
      "defaultValue": 0,
      "description": "应用是否输出元信息"
    },
    {
      "name": "solon.env",
      "type": "java.lang.String",
      "description": "应用环境配置"
    }
  ]
}